    "@vitest/browser": "^3.2.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "~8.2.2",
    "convex-test": "^0.0.37",
    "msw": "^2.10.1",
    "playwright": "^1.52.0",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
